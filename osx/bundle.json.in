{
  "name": "Gitg",

  "variables": {
    "version": "@VERSION@",
    "prefix": "@prefix@"
  },

  "main": "${resources}/bin/gitg",

  "binaries": {
    "${prefix}/bin/gitg": "${resources}/bin/gitg"
  },

  "data": {
    "${prefix}/lib/girepository-1.0/*.typelib": "${resources}/lib/girepository-1.0/"
  },

  "data_interpolated": {
    "${rootdir}/data/Info.plist": "${contents}/Info.plist"
  }
}

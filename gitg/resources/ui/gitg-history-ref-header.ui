<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.3 -->
  <!-- interface-requires gitg 0.0 -->
  <!-- interface-requires gd 1.0 -->
  <template class="GitgHistoryRefHeader" parent="GtkListBoxRow">
    <property name="visible">True</property>
    <style>
      <class name="sidebar"/>
    </style>
    <child>
      <object class="GitgProgressBin" id="d_progress_bin">
        <property name="visible">True</property>
        <child>
          <object class="GtkBox" id="d_box">
            <property name="visible">True</property>
            <property name="orientation">horizontal</property>
            <property name="spacing">6</property>
            <child>
              <object class="GtkLabel" id="d_label">
                <property name="visible">True</property>
                <property name="margin_top">3</property>
                <property name="margin_bottom">3</property>
                <property name="margin_start">16</property>
                <property name="halign">start</property>
                <property name="hexpand">true</property>
              </object>
            </child>
            <child>
              <object class="GtkImage" id="d_icon">
                <property name="visible">False</property>
                <property name="halign">end</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
<!-- vi:ts=2:et -->

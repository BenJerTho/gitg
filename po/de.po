#, c-format
msgid "Loaded %d revisions in %.2fs"
msgstr "%d Revisionen wurden in %.2fs geladen"

#: ../gitg/gitg-window.c:861
#, c-format
msgid "Loading %d revisions..."
msgstr "%d Revisionen werden geladen …"

#: ../gitg/gitg-window.c:870

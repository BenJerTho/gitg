#: ../gitg/gitg-branch-actions.c:1182 ../gitg/gitg-branch-actions.c:1184
#: ../gitg/gitg-branch-actions.c:1223 ../gitg/gitg-branch-actions.c:1225
#: ../gitg/gitg-branch-actions.c:1259 ../gitg/gitg-branch-actions.c:1261
